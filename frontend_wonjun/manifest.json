{
  "manifest_version": 3,
  "name": "Factwave",
  "version": "1.0.0",
  "description": "A Multiagent System for Fact Checking",
  "minimum_chrome_version": "114",
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": [
    "sidePanel",
    "activeTab",
    "scripting"
  ]
}
